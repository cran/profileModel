citHeader("To cite package 'profileModel' in publications use:")

citEntry(entry="Manual",
    title = {"profileModel: Tools for profiling inference functions for various model classes"},
    author = personList(as.person("Ioannis Kosmidis")),
    year = {2007},
    note = {"R package version 0.5-5"},
    url = {"http://go.warwick.ac.uk/kosmidis/software"},
    textVersion = paste(
      "Ioannis Kosmidis (2007).",
      "profileModel: Tools for profiling inference functions for various model classes.",
      "http://go.warwick.ac.uk/kosmidis/software")
)


